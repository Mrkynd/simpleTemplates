<div class="ui container">
  <h1 class="ui centered teal header">wavesurfer.js Example</h1>
  <div id="waveform"></div>
  <button class="ui button" onclick="wavesurfer.playPause()"><i class="play icon"></i></button>
  <button class="ui button" onclick="wavesurfer.stop()"><i class="stop icon"></i></button>
  <button class="ui button" onclick="wavesurfer.pause()"><i class="pause icon"></i></button>
</div>

<div style="padding: 40px 0 40px 0;"></div>

<div class="ui container">
    <table class="ui single line table">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>E-mail address</th>
          <th>Gender</th>
          <th>Ip Address</th>
        </tr>
      </thead>
    {{#each dummy}}
      <tbody>
        <tr>
          <td>{{this.first_name}}</td>
          <td>{{this.last_name}}</td>
          <td>{{this.email}}</td>
          <td>{{this.gender}}</td>
          <td>{{this.ip_address}}</td>
        </tr>
      </tbody>
    {{/each}}
    </table>
</div>
